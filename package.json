    "build": "tsc",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "author": "Nate Dvorachek",
  "license": "ISC",
  "homepage": "https://github.com/n-dvo-tech/react-tinacms-sanity#readme",
  "bugs": {
    "url": "https://github.com/n-dvo-tech/react-tinacms-sanity/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/n-dvo-tech/react-tinacms-sanity.git"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "@tinacms/react-core": "^0.32.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@tinacms/react-core": "0.32.0",
    "@types/jest": "26.0.15",
    "@types/react": "16.9.53",
    "@types/superagent": "4.1.10",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "eslint": "7.12.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "nock": "13.0.4",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "react": "16.13.1",
    "semantic-release": "^17.2.1",
    "ts-jest": "26.4.2",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "@sanity/client": "2.0.1",
    "superagent": "6.1.0"
